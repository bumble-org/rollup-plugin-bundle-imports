{
  "name": "rollup-plugin-bundle-imports",
  "version": "1.0.1",
  "description": "Bundle imports separately and use the result as a file path or a string of code.",
  "repository": "bumble-org/rollup-plugin-bundle-imports",
  "license": "MIT",
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "files": [
    "lib",
    "index.d.ts"
  ],
  "main": "lib/bundle-imports-cjs.js",
  "module": "lib/bundle-imports-esm.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "preversion": "npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "clearMocks": true,
    "modulePathIgnorePatterns": [
      "fixtures/dest"
    ],
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "dependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-pluginutils": "^2.8.1"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "dedupe": "^3.0.1",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "sucrase": "^3.10.1"
  },
  "types": "index.d.ts"
}
